{
  "manifest_version": 2,
  "name": "Points Radar",
  "author": "Kyle Ratajczak",
  "version": "1.0.0",
  "description": "temp",
  "browser_action": {
    "default_popup": "/index.html",
    "default_icon": "logo512.png"
  },
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "icons": {
    "512": "logo512.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'sha256-fdAfMKZtCTRPKMRznGwonJHC++vCCrQS7XFnVrzSXAE=' 'sha256-YD8w9VLrdvqI5/tFVmpT+1337ly+e/JvPrkRxYZo/w4='; object-src 'self';"
}
